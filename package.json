{
  "name": "alpaca-utils",
  "version": "1.0.8",
  "description": "An extra layer of abstraction on top of the Alpaca SDK with defaults and logging for many use cases.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "start": "npm run build && node lib/scratch-space/index.js",
    "test": "jest",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm test",
    "version": "npm run format && git add -A src"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tclare/alpaca-utils.git"
  },
  "keywords": [
    "Alpaca",
    "Utils"
  ],
  "author": "Tommy Clare",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tclare/alpaca-utils/issues"
  },
  "homepage": "https://github.com/tclare/alpaca-utils#readme",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.89",
    "@types/bluebird": "^3.5.36",
    "@types/jest": "^27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.8",
    "@types/ws": "^8.2.2",
    "jest": "^27.4.6",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "@master-chief/alpaca": "^6.3.10",
    "bluebird": "^3.7.2",
    "date-fns": "^2.28.0",
    "date-fns-tz": "^1.2.2",
    "lodash": "^4.17.21"
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!@master-chief/alpaca)"
    ]
  }
}
